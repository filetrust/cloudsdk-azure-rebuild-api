{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post",
        "put"
      ],
      "route": "{version}/{controller}/{action?}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }    
  ],
  "scriptFile": "../dist/rebuild-v1/index.js"
}